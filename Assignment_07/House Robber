class Solution {
    public int rob(int[] arr) {
        //tabulation
        int n=arr.length;
        int[]dp=new int[n];
        if(n==1) return arr[0];
        else if(n==2) return Math.max(arr[0],arr[1]);
        dp[0]=arr[0];
        dp[1]=Math.max(arr[0],arr[1]);
        for(int i=2;i<n;i++){
            dp[i]=Math.max(arr[i]+dp[i-2],dp[i-1]);
        }
        return dp[n-1];
    }
}

//TC=O(N)
//SC=O(N)

